# FastAgent Secrets Configuration
# Copy this file to fastagent.secrets.yaml and fill in your API keys

# Anthropic Configuration (Primary - used by default)
anthropic:
  api_key: "your-anthropic-api-key-here"

# OpenAI Configuration
openai:
  api_key: "your-openai-api-key-here"
  organization: "your-org-id-here"  # Optional

# Google/Vertex AI Configuration
google:
  api_key: "your-google-api-key-here"
  project_id: "your-gcp-project-id"  # For Vertex AI

# Azure OpenAI Configuration
azure:
  api_key: "your-azure-api-key-here"
  endpoint: "https://your-resource.openai.azure.com"
  api_version: "2023-12-01-preview"

# Ollama Configuration (if using local models)
ollama:
  base_url: "http://localhost:11434"
  
# Other LLM Provider Keys
deepseek:
  api_key: "your-deepseek-api-key-here"
  
groq:
  api_key: "your-groq-api-key-here"

# MCP Server Authentication (if needed)
mcp_auth:
  xgboost_server:
    # Add any authentication tokens if your MCP server requires them
    # token: "your-mcp-server-token"
    
# Environment Variables
env:
  # Any environment variables your agents might need
  # HUGGING_FACE_TOKEN: "your-hf-token"
  # GITHUB_TOKEN: "your-github-token" 